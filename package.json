{
  "name": "@c3toronto/n8n-nodes-pco",
  "version": "1.0.0",
  "description": "n8n node to connect with Planning Center Online",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/c3toronto/n8n-nodes-pco#readme",
  "author": {
    "name": "Sejin Kim",
    "email": "redacted@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/c3toronto/n8n-nodes-pco.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "test": "jest",
    "format": "prettier nodes credentials --write",
